@inproceedings{rajala_pulse_2017,
	address = {Seogwipo},
	title = {Pulse arrival time ({PAT}) measurement based on arm {ECG} and finger {PPG} signals - comparison of {PPG} feature detection methods for {PAT} calculation},
	isbn = {978-1-5090-2809-2},
