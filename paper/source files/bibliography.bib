@article{nardelli_assessing_2020,
	title = {Assessing the {Quality} of {Heart} {Rate} {Variability} {Estimated} from {Wrist} and {Finger} {PPG}: {A} {Novel} {Approach} {Based} on {Cross}-{Mapping} {Method}},
