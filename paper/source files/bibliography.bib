@article{drezner_electrocardiographic_2013,
	title = {Electrocardiographic interpretation in athletes: the ‘{Seattle} {Criteria}’: {Table} 1},
	volume = {47},
	issn = {0306-3674, 1473-0480},
	shorttitle = {Electrocardiographic interpretation in athletes},
	url = {http://bjsm.bmj.com/lookup/doi/10.1136/bjsports-2012-092067},
	doi = {10.1136/bjsports-2012-092067},
	abstract = {Sudden cardiac death (SCD) is the leading cause of death in athletes during sport. Whether obtained for screening or diagnostic purposes, an ECG increases the ability to detect underlying cardiovascular conditions that may increase the risk for SCD. In most countries, there is a shortage of physician expertise in the interpretation of an athlete’s ECG. A critical need exists for physician education in modern ECG interpretation that distinguishes normal physiological adaptations in athletes from abnormal ﬁndings suggestive of pathology. On 13–14 February 2012, an international group of experts in sports cardiology and sports medicine convened in Seattle, Washington, to deﬁne contemporary standards for ECG interpretation in athletes. The objective of the meeting was to develop a comprehensive training resource to help physicians distinguish normal ECG alterations in athletes from abnormal ECG ﬁndings that require additional evaluation for conditions associated with SCD.},
	language = {en},
	number = {3},
	urldate = {2018-08-22},
	journal = {British Journal of Sports Medicine},
	author = {Drezner, Jonathan A and Ackerman, Michael John and Anderson, Jeffrey and Ashley, Euan and Asplund, Chad A and Baggish, Aaron L and Börjesson, Mats and Cannon, Bryan C and Corrado, Domenico and DiFiori, John P and Fischbach, Peter and Froelicher, Victor and Harmon, Kimberly G and Heidbuchel, Hein and Marek, Joseph and Owens, David S and Paul, Stephen and Pelliccia, Antonio and Prutkin, Jordan M and Salerno, Jack C and Schmied, Christian M and Sharma, Sanjay and Stein, Ricardo and Vetter, Victoria L and Wilson, Mathew G},
	month = feb,
	year = {2013},
	pages = {122--124},
}

@article{prutkin_electrocardiography_2018,
	title = {Electrocardiography in athletes: normal and abnormal findings},
	issn = {1355-6037, 1468-201X},
	shorttitle = {Electrocardiography in athletes},
	url = {http://heart.bmj.com/lookup/doi/10.1136/heartjnl-2017-312901},
	doi = {10.1136/heartjnl-2017-312901},
	abstract = {Many sporting organisations recommend a preparticipation ECG to screen for disorders which predispose to sudden cardiac arrest (SCA). The ability of the ECG to perform accurately is dependent on the ECG criteria used and the experience of the operator. There have been several ECG criteria over the last decade, though these were recently superseded with the publication of the ’International Consensus Criteria for ECG Interpretation in Athletes’. These criteria use the latest evidence to improve specificity while maintaining sensitivity for ECG-detectable pathologies associated with SCA. Accordingly, this review describes the normal, borderline and abnormal ECG findings in an asymptomatic athlete aged 12–35 years.},
	language = {en},
	urldate = {2018-08-22},
	journal = {Heart},
	author = {Prutkin, Jordan M and Wilson, Mathew G},
	month = aug,
	year = {2018},
	pages = {heartjnl--2017--312901},
}

