@article{tanaka_systematic_nodate,
	title = {Systematic {Review} and {Meta}-{Analysis} of {Diagnostic} {Accuracy} to {Identify} {ST}-{Segment} {Elevation} {Myocardial} {Infarction} on {Interpretations} of {Prehospital} {Electrocardiograms}},
