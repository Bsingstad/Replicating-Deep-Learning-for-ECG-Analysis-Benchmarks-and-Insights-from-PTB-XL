	keywords = {Adult, Aged, Burst–suppression, Cardiac arrest, Coma, EEG, EEG background reactivity, Electroencephalography, Female, Heart Arrest, Hospital Mortality, Humans, Hypothermia, Induced, Hypoxia-Ischemia, Brain, Male, Middle Aged, Neurophysiological Monitoring, Predictive Value of Tests, Prognosis, Prospective Studies, ROC Curve, Status Epilepticus, Status epilepticus},
