	pmid = {20562694},
	keywords = {Aged, Electroencephalography, Female, Heart Arrest, Humans, Hypothermia, Induced, Male, Middle Aged, Predictive Value of Tests, Prognosis, Prospective Studies, Treatment Outcome},
