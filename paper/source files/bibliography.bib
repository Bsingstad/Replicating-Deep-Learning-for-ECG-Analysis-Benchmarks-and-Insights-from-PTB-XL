@article{tofteberg_qt_nodate,
	title = {{QT} {Interval} {Detection} {Using} {Discrete} {Wavelet} {Transform}},
	abstract = {Long QT syndrom (LQTS) is a heart disorder that can cause uncontrollable arrhythmias and in some cases sudden death on healthy individuals[1]. LQTS can be an inherited disorder or provoked due to medication for treating of other medical conditions[2]. The arrhythmias can occur during exercise, as a reaction to stress or when sleeping[1]. The disorder origins from abnormal electrical signalling in the heart’s lower chambers, the ventricles[1]. In order to estimate the QT interval, the QRS onset and T offset needs to be detected in the ECG signal. A study of how the features from multilevel Discrete Wavelet Transform can be used for estimating the QT inteval has been further examined. The results shows that the algorithm developed misses the QRS onset with a mean distance of 2.53 samples, and the T offset with 9.25 samples, on a great variety of ECG morphology.},
	language = {en},
	author = {Tofteberg, SW and Moen, U},
	pages = {5},
}

