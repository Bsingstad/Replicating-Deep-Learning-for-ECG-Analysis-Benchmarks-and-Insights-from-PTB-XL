	number = {11},
	urldate = {2020-12-16},
	journal = {Symmetry},
	author = {Li, Dengao and Wu, Hang and Zhao, Jumin and Tao, Ye and Fu, Jian},
	month = nov,
	year = {2020},
	note = {Number: 11
Publisher: Multidisciplinary Digital Publishing Institute},
	keywords = {12-leads electrocardiogram, arrhythmia classification system, attention mechanism, deep neural network},
	pages = {1827},
}

@inproceedings{chen_large-scale_2019,
	title = {Large-scale {Classification} of 12-lead {ECG} with {Deep} {Learning}},
