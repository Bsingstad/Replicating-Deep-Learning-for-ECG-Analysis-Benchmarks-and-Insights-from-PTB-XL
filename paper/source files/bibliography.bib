	author = {Min, Seonwoo and Choi, Hyun-Soo and Han, Hyeongrok and Seo, Minji and Kim, Jin-Kook and Park, Junsang and Jung, Sunghoon and Oh, Il-Young and Lee, Byunghan and Yoon, Sungroh},
